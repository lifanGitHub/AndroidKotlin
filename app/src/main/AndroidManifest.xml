<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kotlin.lifan.androidkotlin">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".item1.RecyclerActivity"
            android:theme="@style/RecyclerActivityTheme"/>

        <activity android:name=".item1.StaggeredActivity" />
        <activity android:name=".service.ServiceActivity"/>
        <activity android:name=".service.demo1.My1Activity"/>
        <activity android:name=".service.demo2.My2Activity"/>
        <activity android:name=".service.demo3.My3Activity"/>
        <activity android:name=".service.demo4.My4Activity"/>
        <activity android:name=".service.demo5.My5Activity"/>
        <activity android:name=".service.demo6.My6Activity"/>
        <service android:name=".service.demo1.My1Service"/>
        <service android:name=".service.demo1.My12Service"/>
        <service android:name=".service.demo2.My2Service"/>
        <service android:name=".service.demo3.My3Service"/>
        <service android:name=".service.demo4.My4Service"/>
        <service android:name=".service.demo5.BroadcastService"
            android:process=":remote"/>
        <service android:name=".service.demo6.My6Service"
            android:process=":remote"/>
        <receiver android:name=".service.demo5.NormalReceiver">
            <intent-filter>
                <action android:name="com.example.normal.receiver" />
            </intent-filter>
        </receiver>

    </application>

</manifest>